{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./dist/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "lib": ["es6"],
    "moduleResolution": "Node"
  },
  "include": ["./src/**/**/*"]
}
